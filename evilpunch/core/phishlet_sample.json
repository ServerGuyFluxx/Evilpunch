  "auth_urls": [
    "/session",
    "/register"
  ],
  "credentials": [
    {
      "type": "json",
      "name": "username",
      "keyword": "login",
      "regexp": "true",
      "regexp_string": "login"
    },
    {
      "type": "json",
      "name": "username",
      "keyword": "login",
      "regexp": "true",
      "regexp_string": "login\":\\s*\"([^\"]*)\""
    },
    {
      "type": "post",
      "name": "password",
      "keyword": "password",
      "regexp": "false"
    }
  ],
  "cookies": [
    "sessionid",
    "csrftoken",
    "*"
  ],
  "inject_js": [
